{
    "name": "hkirsman/google-search-console",
    "description": "Wrapper for google/apiclient to access and retrieve data from Google Search Console.",
    "require": {
        "google/apiclient": "^2.0.0@RC"
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Webmasters"
        ]
    },
    "authors": [
        {
            "name": "Hannes Kirsman",
            "email": "hannes.kirsman@eesti.ee"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HannesKirsman\\GoogleSearchConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "HannesKirsman\\GoogleSearchConsole\\Tests": "tests"
        }
    }
}